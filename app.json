{
  "expo": {
    "scheme": "acme",
    "icon": "./assets/images/icon.png",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "name": "Expenses",
    "slug": "react-native-tracker",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "06b512e1-6ff2-410a-a0ef-2ef8aaa0f741"
      }
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE"
      ],
      "package": "com.pruthvesh.reactnativetracker"
    }
  }
}